{
  "name": "react-nodejs-webpack",
  "version": "1.0.0",
  "description": "hoho",
  "type": "module",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev:client": "webpack --watch --config configs/webpack.dev.client.js",
    "dev:server": "webpack --watch --config configs/webpack.dev.server.js",
    "dev:run_server": "nodemon dist/server.js",
    "dev": "run-p dev:*",
    "build": "rimraf build && webpack --config configs/webpack.prod.client.js && webpack --config configs/webpack.prod.server.js",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc"
  },
  "author": "DuCanh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@swc/core": "1.3.4",
    "@types/common-tags": "1.8.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.14",
    "@types/fs-extra": "9.0.13",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "clean-css": "5.3.1",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.2.0",
    "cssnano": "5.1.13",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "2.6.1",
    "node-sass": "7.0.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.17",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-jsdoc": "0.4.2",
    "rimraf": "3.0.2",
    "sass": "1.55.0",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "swc-loader": "0.2.3",
    "terser": "5.15.0",
    "terser-webpack-plugin": "5.3.6",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-webpack-plugin": "6.5.4"
  },
  "dependencies": {
    "common-tags": "1.8.2",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.3",
    "react-router-dom": "6.4.1"
  }
}
