{
  "name": "react-nodejs-webpack",
  "version": "1.0.0",
  "description": "hoho",
  "sideEffects": [
    "*.css",
    "*.sass",
    "*.scss"
  ],
  "type": "module",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "analyze": "cross-env ANALYZER=true pnpm build",
    "dev:client": "webpack --watch --config configs/dev/webpack.client.js",
    "dev:server": "webpack --watch --config configs/dev/webpack.server.js",
    "dev:run_server": "nodemon",
    "dev": "run-p dev:*",
    "build:client": "webpack --mode=production --config configs/prod/webpack.client.js",
    "build:server": "webpack --mode=production --config configs/prod/webpack.server.js",
    "build": "rimraf build && run-s build:client build:server",
    "format": "prettier --write",
    "lint-staged": "lint-staged",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "typecheck": "tsc"
  },
  "author": "DuCanh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@swc/core": "1.3.22",
    "@types/clean-css": "4.2.6",
    "@types/common-tags": "1.8.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.14",
    "@types/fs-extra": "9.0.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/shell-quote": "1.7.1",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "clean-css": "5.3.1",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.2",
    "css-minimizer-webpack-plugin": "4.2.2",
    "cssnano": "5.1.14",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "file-loader": "6.2.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "2.7.2",
    "node-sass": "8.0.0",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "postcss-loader": "7.0.2",
    "prettier": "2.8.1",
    "prettier-plugin-jsdoc": "0.4.2",
    "rimraf": "3.0.2",
    "sass": "1.56.2",
    "sass-loader": "13.2.0",
    "shell-quote": "1.7.4",
    "style-loader": "3.3.1",
    "swc-loader": "0.2.3",
    "terser": "5.16.1",
    "terser-webpack-plugin": "5.3.6",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-cli": "5.0.1",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-webpack-plugin": "6.5.4"
  },
  "dependencies": {
    "common-tags": "1.8.2",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "fs-extra": "11.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.4",
    "react-router-dom": "6.4.5"
  }
}
