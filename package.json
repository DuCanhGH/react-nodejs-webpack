{
  "name": "react-nodejs-webpack",
  "version": "1.0.0",
  "description": "hoho",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "concurrently --handle-input -n wp-client,wp-server,nodemon \"webpack --watch --config configs/webpack.dev.client.js\" \"webpack --watch --config configs/webpack.dev.server.js\" \"nodemon dist/server.js\"",
    "build": "webpack --config configs/webpack.prod.client.js && webpack --config configs/webpack.prod.server.js",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc"
  },
  "author": "DuCanh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.18.6",
    "@swc/core": "1.2.218",
    "@types/common-tags": "1.8.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.13",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "clean-css": "5.3.1",
    "compression-webpack-plugin": "10.0.0",
    "concurrently": "7.2.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "cssnano": "5.1.12",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "2.6.1",
    "node-sass": "7.0.1",
    "nodemon": "2.0.19",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.3",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-node-externals": "3.0.0",
    "workbox-cacheable-response": "6.5.3",
    "workbox-core": "6.5.3",
    "workbox-expiration": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-strategies": "6.5.3",
    "workbox-webpack-plugin": "6.5.3"
  },
  "dependencies": {
    "common-tags": "1.8.2",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  }
}
