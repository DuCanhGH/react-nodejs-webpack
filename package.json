{
  "name": "react-nodejs-webpack",
  "version": "1.0.0",
  "description": "A setup of React with Webpack. Don't use in production.",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.sass",
    "*.scss"
  ],
  "type": "module",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "analyze": "cross-env ANALYZER=true pnpm build",
    "dev:client": "webpack serve --config configs/dev/webpack.client.js --mode=development",
    "dev:server": "webpack --config configs/dev/webpack.server.js --mode=development --watch",
    "dev:run_server": "nodemon",
    "dev": "run-p dev:*",
    "build:client": "webpack --mode=production --node-env production --config configs/prod/webpack.client.js",
    "build:server": "webpack --mode=production --node-env production --config configs/prod/webpack.server.js",
    "build": "rimraf build && run-s build:client build:server",
    "format": "prettier --write",
    "lint-staged": "lint-staged",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "typecheck": "tsc"
  },
  "author": "DuCanhGH",
  "license": "MIT",
  "dependencies": {
    "@remix-run/node": "1.14.3",
    "@remix-run/router": "1.4.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "fs-extra": "11.1.0",
    "husky": "8.0.3",
    "react": "0.0.0-experimental-47cf4e578-20230315",
    "react-dom": "0.0.0-experimental-47cf4e578-20230315",
    "react-markdown": "8.0.5",
    "react-router-dom": "6.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.21.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@swc/core": "1.3.41",
    "@types/clean-css": "4.2.6",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/shell-quote": "1.7.1",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-env": "1.18.0",
    "@types/webpack-hot-middleware": "2.25.6",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "clean-css": "5.3.2",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "cssnano": "5.1.15",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "file-loader": "6.2.0",
    "lint-staged": "13.2.0",
    "mini-css-extract-plugin": "2.7.5",
    "node-sass": "8.0.0",
    "nodemon": "2.0.21",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-loader": "7.1.0",
    "prettier": "2.8.4",
    "prettier-plugin-jsdoc": "0.4.2",
    "react-refresh": "0.14.0",
    "rimraf": "4.4.0",
    "sass": "1.59.3",
    "sass-loader": "13.2.0",
    "shell-quote": "1.8.0",
    "style-loader": "3.3.2",
    "swc-loader": "0.2.3",
    "terser": "5.16.6",
    "terser-webpack-plugin": "5.3.7",
    "type-fest": "3.6.1",
    "typescript": "5.1.0-dev.20230318",
    "webpack": "5.76.2",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-middleware": "6.0.1",
    "webpack-dev-server": "4.13.0",
    "webpack-hot-middleware": "2.25.3",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
